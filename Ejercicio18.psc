Algoritmo Ejercicio18
	
	Definir N1, N2 Como Real;
	
	Escribir "Ingrese el primer número";
	
	Leer N1;
	
	Escribir "Ingrese el segundo número";
	
	Leer N2;
	
	Escribir "El cociente es ", (N1/N2), " Y el valor redondeado es ", (redon(N1/N2)), " Y el valor truncado es ", (trunc(N1/N2));
	
FinAlgoritmo
