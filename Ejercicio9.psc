Algoritmo Ejercicio9
	
	Definir A, B Como Entero;
    
    Escribir "Ingrese el numero A";
    Leer A;
    Escribir "Ingrese el numero B";
    Leer B;
	
    Escribir "A es mayor que B = ", (A>B);
    Escribir "A es menor que B = ", (A<B);
    Escribir "A es igual a B = ", (A==B);
    Escribir "A es distinto que B = ", (A!=B);
	
FinAlgoritmo
