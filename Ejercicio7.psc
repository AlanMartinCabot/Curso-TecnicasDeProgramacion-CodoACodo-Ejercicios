Algoritmo Ejercicio7
	
	Definir Base, Altura como Entero;
    
    Escribir "Ingrese la base del rectangulo";
    Leer Base;
    Escribir "Ingrese la altura del rectangulo";
    Leer Altura;
	
    Escribir "La superficie del rectangulo es ", (Base*Altura);
    Escribir "El perimetro del rectangulo es ", (Base*2) + (Altura*2);
	
FinAlgoritmo
